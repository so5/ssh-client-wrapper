{
  "name": "ssh-client-wrapper",
  "version": "3.0.0",
  "description": "wrapper library for open ssh client and rsync",
  "keywords": [
    "ssh",
    "ssh2",
    "secure",
    "shell",
    "client",
    "wrapper",
    "rsync"
  ],
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "mocha": "mocha --recursive test",
    "lint": "eslint --fix ./",
    "test": "c8 --reporter=html --reporter=text mocha --recursive test",
    "coverage": "c8 report --reporter=text-lcov > coverage-report.lcov",
    "postversion": "git push && git push origin $(git describe --abbrev=0 --tags)",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Naoyuki Sogo <sogo@longtail-software.co.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/so5/ssh-client-wrapper"
  },
  "homepage": "https://github.com/so5/ssh-client-wrapper#readme",
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "dependencies": {
    "node-pty": "^0.10.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "debug": "^4.4.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "glob": "^10.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@stylistic/eslint-plugin": "^3.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-node": "^11.1.0",
    "fs-extra": "^11.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "mocha": "^10.2.0",
    "c8": "^10.1.3",
    "globals": "^16.3.0",
    "sinon": "^15.0.3",
    "sinon-chai": "^3.7.0"
  }
}
